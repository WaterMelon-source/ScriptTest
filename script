local Gamepass1Toggle = Tabs.Gamepasses:AddToggle("Gamepass1", {Title = "🛏   Premium Room", Default = game:GetService("Players").LocalPlayer.gamepasses.premium_room.Value })

Gamepass1Toggle:OnChanged(function(Value)
	game:GetService("Players").LocalPlayer.gamepasses.premium_room.Value = Value
end)

