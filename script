local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

if dsdsd then
	OrionLib:MakeNotification({
		Name = "OrionLib - Notification",
		Content = "Successfully loaded script!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
else
	local Window = OrionLib:MakeWindow({Name = "[Update] The Flash: Project Speedforce", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})
	local character = game.Players.LocalPlayer.Character
	if character.HumanoidRootPart:FindFirstChild("Smoke") and character.HumanoidRootPart:FindFirstChild("Dash") then
		print("have smoke and dash")
	else
		local SmokeClone = game.ReplicatedStorage.Effects.Smoke:Clone()
		SmokeClone.Parent = character.HumanoidRootPart
		SmokeClone.Rate = 10
		SmokeClone.Lifetime = NumberRange.new(0.4, 0.4)
		SmokeClone.Enabled = false
		local DashClone = game.ReplicatedStorage.Sounds.Dash:Clone()
		DashClone.Parent = character.HumanoidRootPart
		DashClone.Volume = 3
	end
	local Smoke = character.HumanoidRootPart.Smoke
	local Dash = character.HumanoidRootPart.Dash
	local StaminaValue = 100
	local StaminaToggle = true
	local TachyonToggle = false

	local Tab = Window:MakeTab({
		Name = "Main",
		Icon = "rbxassetid://4483345998",
		PremiumOnly = false
		})

	local Section = Tab:AddSection({
		Name = "Player"
		})

	local SpeedShow = Tab:AddLabel("Your Speed: "..tostring(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed))
	local StaminaShow = Tab:AddLabel("Your Stamina: "..tostring(character:WaitForChild("Speedster").Config.Stamina.Value))

	Tab:AddSlider({
		Name = "Max Speed",
		Min = 0,
		Max = 10000,
		Default = 400,
		Color = Color3.fromRGB(255, 89, 89),
		Increment = 100,
		ValueName = "Speed",
		Callback = function(Value)
			character:WaitForChild("Speedster").Config.Speed.Value = Value
		end    
	})

	Tab:AddSlider({
		Name = "Stamina",
		Min = 0,
		Max = 10000,
		Default = 100,
		Color = Color3.fromRGB(9, 137, 207),
		Increment = 10,
		ValueName = "Stamina",
		Callback = function(Value)
			character:WaitForChild("Speedster").Config.Stamina.Value = Value
			StaminaValue = Value
		end    
	})

	Tab:AddToggle({
		Name = "Stamina waste",
		Default = true,
		Callback = function(Value)
			StaminaToggle = Value
		end    
	})

	Tab:AddDropdown({
		Name = "Speedforce",
		Default = "Normal",
		Options = {"Normal", "Negative", "Elemental"},
		Callback = function(Value)
			if Value == "Negative" then
				character:WaitForChild("Speedster").Config.Negative.Value = true
				character:WaitForChild("Speedster").Config.Elemental.Value = false
			elseif Value == "Elemental" then
				character:WaitForChild("Speedster").Config.Elemental.Value = true
				character:WaitForChild("Speedster").Config.Negative.Value = false
			else
				character:WaitForChild("Speedster").Config.Negative.Value = false
				character:WaitForChild("Speedster").Config.Elemental.Value = false
			end
		end    
	})

	local Section = Tab:AddSection({
		Name = "Game"
		})

	Tab:AddButton({
		Name = "Show players names in Flash Time",
		Callback = function()
			for i, v in pairs(game.ReplicatedStorage.InFlashtime:GetChildren()) do
				OrionLib:MakeNotification({
						Name = "In Flash Time Player:",
						Content = tostring(v),
						Image = "rbxassetid://4483345998",
						Time = 2
				})
			end
		end
	})

	OrionLib:MakeNotification({
		Name = "OrionLib - Notification",
		Content = "Successfully loaded script!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})

	character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
		if TachyonToggle == true then
			SpeedShow:Set("Your Speed: "..tostring(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed).." + 4x Tachyon")
		else
			SpeedShow:Set("Your Speed: "..tostring(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed))
		end
	end)

	character:WaitForChild("Speedster").Config.Stamina:GetPropertyChangedSignal("Value"):Connect(function()
		wait(0.1)
		StaminaShow:Set("Your Stamina: "..tostring(character:WaitForChild("Speedster").Config.Stamina.Value))
		if StaminaToggle == false then
			character:WaitForChild("Speedster").Config.Stamina.Value = StaminaValue
		end
	end)

	OrionLib:Init()
end
