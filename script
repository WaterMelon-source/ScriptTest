local SpeedSlider = Tabs.Player:AddSlider("Slider", {
    Title = "ğŸ‘Ÿ   Max Speed",
    Description = "\nChange Player's Max Speed",
    Default = 100,
    Min = 0,
    Max = 10000,
    Rounding = 100,
    Callback = function(Value)
        character:WaitForChild("Speedster").Config.Speed.Value = Value
    end
})

Tabs.Player:AddButton({
    Title = "ğŸ”„  Set to default",
    --Description = "",
    Callback = function()
        character:WaitForChild("Speedster").Config.Speed.Value = 100
	SpeedSlider:SetValue(100)
    end
})

local StaminaSlider = Tabs.Player:AddSlider("Slider", {
    Title = "âš¡  Stamina",
    Description = "\nChange Player's Stamina",
    Default = 100,
    Min = 0,
    Max = 10000,
    Rounding = 100,
    Callback = function(Value)
        character:WaitForChild("Speedster").Config.Stamina.Value = Value
	StaminaValue = Value
    end
})

Tabs.Player:AddButton({
    Title = "ğŸ”„  Set to default",
    --Description = "",
    Callback = function()
        character:WaitForChild("Speedster").Config.Stamina.Value = 100
	StaminaValue = 100
	StaminaSlider:SetValue(100)
    end
})

local StaminaWToggle = Tabs.Player:AddToggle("StaminaW", {Title = "âš¡   Stamina Waste", Default = true })

StaminaWToggle:OnChanged(function(Value)
	StaminaToggle = Value
end)

character:WaitForChild("Speedster").Config.Stamina:GetPropertyChangedSignal("Value"):Connect(function()
	wait(0.1)
	if StaminaToggle == false then
		character:WaitForChild("Speedster").Config.Stamina.Value = StaminaValue
	end
end)

local Dropdown = Tabs.Features:AddDropdown("Dropdown", {
	Title = "Dropdown",
	Values = {"Normal", "Negative", "Elemental"},
	Multi = false,
	Default = 1,
})

Dropdown:OnChanged(function(Value)
	if Value == "Negative" then
		character:WaitForChild("Speedster").Config.Negative.Value = true
		character:WaitForChild("Speedster").Config.Elemental.Value = false
	elseif Value == "Elemental" then
		character:WaitForChild("Speedster").Config.Elemental.Value = true
		character:WaitForChild("Speedster").Config.Negative.Value = false
	else
		character:WaitForChild("Speedster").Config.Negative.Value = false
		character:WaitForChild("Speedster").Config.Elemental.Value = false
	end
end)

-------------

Window:SelectTab(1)

Notify("âœ…  Successfully Loaded!", 5)
