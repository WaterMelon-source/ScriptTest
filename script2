	KeyBindsTab:AddBind({
		Name = "üì∑  Disable/Enable Camera Shake KeyBind",
		Default = Enum.KeyCode.L,
		Hold = false,
		Callback = function()
			CameraShake(game.Players.LocalPlayer.PlayerScripts.source.CameraShake.Enabled)
		end    
	})

--------------------------------------------------------------------------------------Settings Tab------------------------------------------------------------------------------------

	local SettingsTab = Window:MakeTab({
		Name = "Settings",
		Icon = "rbxassetid://10734950309",
		PremiumOnly = false
	})

	local Section = SettingsTab:AddSection({
		Name = "‚öô  Settings"
	})

	SettingsTab:AddToggle({
		Name = "üí•  Disable/Enable Fall Damage",
		Default = false,
		Callback = function(Value)
			characterC.FallDamage.Enabled = not Value
			characterC.FallDamage.Disabled = Value
		end    
	})

	Notify("‚úÖ  Successfully loaded script!", 5)

	OrionLib:Init()
end

---------------------------------------------------------------------------------------Values-------------------------------------------------------------------------------------

function Values(humanoid)
	humanoid:GetPropertyChangedSignal("Health"):Connect(function()
		_G.HealthShow:Set("‚ù§  Health: "..tostring(humanoid.Health))
	end)

	humanoid:GetAttributeChangedSignal("Energy"):Connect(function()
		_G.EnergyShow:Set("‚ö°  Energy: "..tostring(humanoid:GetAttribute("Energy")))
	end)

	humanoid:GetAttributeChangedSignal("Hunger"):Connect(function()
		_G.HungerShow:Set("üçñ  Hunger: "..tostring(humanoid:GetAttribute("Hunger")))
	end)

	humanoid:GetPropertyChangedSignal("MaxHealth"):Connect(function()
		_G.MaxHealthShow:Set("üíï  Max Health: "..tostring(humanoid.MaxHealth))
	end)

	humanoid:GetAttributeChangedSignal("MaxEnergy"):Connect(function()
		_G.MaxEnergyShow:Set("üå©  Max Energy: "..tostring(humanoid:GetAttribute("MaxEnergy")))
	end)

	humanoid:GetAttributeChangedSignal("MaxHunger"):Connect(function()
		_G.MaxHungerShow:Set("üòã  Max Hunger: "..tostring(humanoid:GetAttribute("MaxHunger")))
	end)

	humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
		_G.SpeedShow:Set("üëü  Speed: "..tostring(humanoid.WalkSpeed))
	end)

	humanoid:GetPropertyChangedSignal("JumpPower"):Connect(function()
		_G.JumpShow:Set("ü¶µ  Jump: "..tostring(humanoid.JumpPower))
	end)

	game.ReplicatedStorage.ServerSettings.TimeSettings.TimeLeft:GetPropertyChangedSignal("Value"):Connect(function()
		if game.Lighting.ClockTime == 0 and _G.FullBrightEnabled == false then
			_G.GameStatusShow:Set("üåô  Game Status: Night, "..game.ReplicatedStorage.ServerSettings.TimeSettings.TimeLeft.Value)
		elseif game.Lighting.ClockTime == 12 and _G.FullBrightEnabled == false then
			_G.GameStatusShow:Set("üåû  Game Status: Day, "..game.ReplicatedStorage.ServerSettings.TimeSettings.TimeLeft.Value)
		else
			_G.GameStatusShow:Set("üîÜ  Game Status: FullBright")
		end
	end)
end

game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(character)
	local humanoid = character:WaitForChild("Humanoid")
	Values(humanoid)
end)

local humanoid = characterC:WaitForChild("Humanoid")
Values(humanoid)

--------------------------------------------------------------------------------------Key System------------------------------------------------------------------------------------

if gg then--game.CoreGui:FindFirstChild("Orion") then
	warn("‚ö†Ô∏è Dash - Notification: Script already loaded!")
else
	local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

	local Window = OrionLib:MakeWindow({Name = "üîí  Dash ‚Ä¢ Key System", IntroEnabled = true, IntroText = "Dash ‚Ä¢ Key System", IntroIcon = "http://www.roblox.com/asset/?id=257841248", HidePremium = false, SaveConfig = true, ConfigFolder = "Dash_Key"})

	function Notify(content, time)
		OrionLib:MakeNotification({
			Name = "Dash - Notification",
			Content = content,
			Image = "http://www.roblox.com/asset/?id=12684119225",
			Time = time
		})
	end

	local KeyTab = Window:MakeTab({
		Name = "Key",
		Icon = "http://www.roblox.com/asset/?id=10359759953",
		PremiumOnly = false
	})

	KeyTab:AddTextbox({
		Name = "üîë  Key: ",
		Default = _G.key,
		TextDisappear = false,
		Callback = function(Value)
			_G.key = Value
		end	  
	})

	KeyTab:AddButton({
		Name = "‚úÖ  Submit",
		Callback = function()
			if _G.key == "Dash_X%eQk]5Q10k2R.WPPQ-v" then
				RunDash()
			else
				Notify("‚ùå  Error: Not Correct Key", 2)
			end
		end
	})

	local Orion = game.CoreGui:FindFirstChild("Orion")
	print(Orion)
	--Orion.Name == "DashKS"

	if _G.key == "Dash_X%eQk]5Q10k2R.WPPQ-v" then
		RunDash()
		Notify("‚úÖ  Key is still valid", 5)
	else
		Notify("‚ö†Ô∏è  Key Expired/Leaved The Game/New", 10)
	end
	OrionLib:Init()
end
