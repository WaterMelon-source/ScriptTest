local SpeedSlider = Tabs.Player:AddSlider("Slider", {
    Title = "üëü   Speed",
    Description = "\nChange Player's Speed",
    Default = 16,
    Min = 0,
    Max = 999,
    Rounding = 1,
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

Tabs.Player:AddButton({
    Title = "üîÑ  Set to default",
    --Description = "",
    Callback = function()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
		SpeedSlider:SetValue(16)
    end
})

local JumpPowerSlider = Tabs.Player:AddSlider("Slider", {
    Title = "ü¶µ  JumpPower",
    Description = "\nChange Player's JumpPower",
    Default = 50,
    Min = 0,
    Max = 999,
    Rounding = 1,
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end
})

Tabs.Player:AddButton({
    Title = "üîÑ  Set to default",
    --Description = "",
    Callback = function()
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
		JumpPowerSlider:SetValue(50)
    end
})

Tabs.Farm:AddParagraph({
    Title = "									   üç∑  Restaurant  üç∑"
    --Content = ""
})

local Input = Tabs.Farm:AddInput("PlayerInput", {
	Title = "üë§  Player Restaurant: ",
	Default = "LocalPlayer",
	Placeholder = "Placeholder",
	Numeric = false, -- Only allows numbers
	Finished = true, -- Only calls callback when you press enter
	Callback = function(Value)
		for i,v in pairs(workspace.Tycoons:GetChildren()) do
			if v:FindFirstChild("Player") and v.Player.Value == game.Players:FindFirstChild(Value) then
				_G.restaurant = v
				_G.CurrentPlayer = game.Players:FindFirstChild(Value).Name
			end
		end
	end
})

Tabs.Farm:AddParagraph({
    Title = "									    üî•  Functions  üî•"
    --Content = ""
})



local AutoCookToggle = Tabs.Farm:AddToggle("AutoCook", {Title = "üë®‚Äçüç≥   Auto Cook", Default = _G.AutoCooking})

AutoCookToggle:OnChanged(function(Value)
	_G.AutoCooking = Value
	while _G.AutoCooking == true do
		wait(0.1)
		for i,v in pairs(workspace.Temp:GetChildren()) do
			if not v:FindFirstChild("Dialog2") and not v:FindFirstChild("QuestInteraction") then
				for i,f in pairs(v:GetChildren()) do
					if f.Name:match(tostring(_G.CurrentPlayer)) then
						fireproximityprompt(f, 1, true)
					end
				end
			end
		end
		for i,v in pairs(_G.restaurant.Items.OftenFiltered.Kitchen:GetChildren()) do
			if v:FindFirstChild("InUse") and v.Base.Attachment:FindFirstChild("Cook_Oven") then
				game:GetService("ReplicatedStorage").Events.NewCookingRemote:FireServer("completeTask", _G.restaurant.Items.OftenFiltered.Kitchen:FindFirstChild("3"), "Oven")
			elseif v:FindFirstChild("InUse") and v.Base.Attachment:FindFirstChild("Cook_ChoppingBoard") then
				game:GetService("ReplicatedStorage").Events.NewCookingRemote:FireServer("completeTask", _G.restaurant.Items.OftenFiltered.Kitchen:FindFirstChild("392"), "ChoppingBoard")
			elseif v:FindFirstChild("InUse") and v.Base.Attachment:FindFirstChild("Cook_Stove") then
				game:GetService("ReplicatedStorage").Events.NewCookingRemote:FireServer("completeTask", _G.restaurant.Items.OftenFiltered.Kitchen:FindFirstChild("393"), "Stove")
			end
		end
		for i,v in pairs(workspace.Waypoints:GetChildren()) do
			if workspace.Waypoints:FindFirstChild("Part") then
				v:Destroy()
			end
		end
	end
end)

local AutoCollectCashToggle = Tabs.Farm:AddToggle("AutoCollectCash", {Title = "üíµ   Auto Collect Cash", Default = _G.AutoCollectCash})

AutoCollectCashToggle:OnChanged(function(Value)
	_G.AutoCollectCash = Value
	while _G.AutoCollectCash == true do
		wait(0.5)
		for i,v in pairs(_G.restaurant.Items.OftenFiltered.Surface:GetChildren()) do
			if v:FindFirstChild("Bill") then
				local args = {
    					[1] = _G.restaurant,
    					[2] = {
        					["name"] = "CollectBill",
        					["model"] = v
    					}
				}

				game:GetService("ReplicatedStorage").Events.ClientTycoonInput:FireServer(unpack(args))
			end
		end
	end
end)

local AutoCollectTrashToggle = Tabs.Farm:AddToggle("AutoCollectTrash", {Title = "üçΩ   Auto Collect Trash", Default = _G.AutoCollectTrash})

AutoCollectTrashToggle:OnChanged(function(Value)
	_G.AutoCollectTrash = Value
	while _G.AutoCollectTrash == true do
		wait(0.5)
		for i,v in pairs(_G.restaurant.Items.OftenFiltered.Surface:GetChildren()) do
			if v:FindFirstChild("Trash") then
				local args = {
    					[1] = _G.restaurant,
    					[2] = {
        					["name"] = "ClearTrash",
        					["model"] = v
    					}
				}

				game:GetService("ReplicatedStorage").Events.ClientTycoonInput:FireServer(unpack(args))
			end
		end
	end
end)

local AutoCollectTipToggle = Tabs.Farm:AddToggle("AutoCollectTip", {Title = "üí∏   Auto Collect Tip", Default = _G.AutoCollectTip})

AutoCollectTipToggle:OnChanged(function(Value)
	_G.AutoCollectTip = Value
	while _G.AutoCollectTip == true do
		wait(5)
		local args = {
    			[1] = _G.restaurant,
    			[2] = {
     	  			["name"] = "CollectTip"
    			}
		}
		game:GetService("ReplicatedStorage").Events.ClientTycoonInput:FireServer(unpack(args))
	end
end)

local AutoSeatToggle1 = Tabs.Farm:AddToggle("AutoSeat", {Title = "ü™ë   Auto Customers Seat", Default = _G.AutoSeat})

AutoSeatToggle1:OnChanged(function(Value)
	_G.AutoSeat = Value
	while _G.AutoSeat == true do
		wait(0.5)
		for i,v in pairs(_G.restaurant.Customers:GetChildren()) do
			if v:FindFirstChild("Data").Stage and v:FindFirstChild("Data").Stage.Value == "Entered" then
				for i,f in pairs(_G.restaurant.Items.AlwaysFiltered.NPCs:GetChildren()) do
					print(tostring(v.Name))
					if f:FindFirstChild(tostring(v.Name)) then
						for i,s in pairs(_G.restaurant.Items.OftenFiltered.Surface:GetChildren()) do
							print(s.Name)
							if s:FindFirstChild("InUse") and s:FindFirstChild("InUse").Value == false then
								_G.table = s
								print(_G.table)
								local args = {
 	   								[1] = _G.restaurant,
	    								[2] = {
 	      									["name"] = "SendToTable",
		       								["obj"] = _G.restaurant.Items.OftenFiltered.Surface:FindFirstChild(tostring(_G.table.Name)),
	        								["group"] = tostring(v.Name),
        									["tycoon"] = _G.restaurant,
        									["customer"] = tostring(v.Name)
    									}
								}

								game:GetService("ReplicatedStorage").Events.ClientTycoonInput:FireServer(unpack(args))
							end
						end
					end
				end
			end
		end
	end
end)

local AutoServeToggle = Tabs.Farm:AddToggle("AutoServe", {Title = "üí¨   Auto Customers Serve", Default = _G.AutoServe})

AutoServeToggle:OnChanged(function(Value)
	_G.AutoServe = Value
	while _G.AutoServe == true do
		wait(0.5)
		for i,v in pairs(_G.restaurant.Customers:GetChildren()) do
			if v:FindFirstChild("Data").Stage and v:FindFirstChild("Data").NumOrders.Value >= 1 then
				for i,f in pairs(_G.restaurant.Items.AlwaysFiltered.NPCs:GetChildren()) do
					if f:FindFirstChild(tostring(v.Name)) then
						for i,s in pairs(f:FindFirstChild(tostring(v.Name)):GetChildren()) do
							local args = {
    								[1] = _G.restaurant,
    								[2] = {
        								["name"] = "ManageCustomers",
        								["group"] = tostring(v.Name),
       									["tycoon"] = _G.restaurant,
        								["customer"] = tostring(s.Name)
    								}
							}
							game:GetService("ReplicatedStorage").Events.ClientTycoonInput:FireServer(unpack(args))
						end
					end
				end
			end
		end
	end
end)

local AutoFoodToggle = Tabs.Farm:AddToggle("AutoFood", {Title = "üçù   Auto Deliver Food", Default = _G.AutoFood})

AutoFoodToggle:OnChanged(function(Value)
	_G.AutoFood = Value
	while _G.AutoFood == true do
		wait(0.5)
		for i,v in pairs(_G.restaurant.Items.OftenFiltered.Surface:GetChildren()) do
			if v:FindFirstChild("OrderedFoods") then
				for i,f in pairs(v:FindFirstChild("OrderedFoods"):GetChildren()) do
					for i,s in pairs(_G.restaurant.Customers:GetChildren()) do
						if s:FindFirstChild("Customers") then
							for i,d in pairs(s.Customers:GetChildren()) do
								if d:FindFirstChild("MealChoice") then
									_G.meal = tostring(d.MealChoice.Value)
									_G.order = tostring(f.Name)
									if _G.meal == _G.order then
										local args = {
    											[1] = _G.restaurant,
    											[2] = {
        											["models"] = {},
        											["name"] = "ManageFood",
       												["model"] = f,
        											["customer"] = tostring(d),
       												["tycoon"] = _G.restaurant,
        											["group"] = tostring(s)
    											}
										}

										game:GetService("ReplicatedStorage").Events.ClientTycoonInput:FireServer(unpack(args))
										wait(1)
									end
								end
							end
						end
					end
					f:Destroy()
				end
			end
		end
	end
end)


