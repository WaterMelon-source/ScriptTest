local VehicleVelocityToggle = Tabs.Vehicle:AddToggle("VehicleVelocityToggle", {Title = "ğŸš™   Vehicle Acceleration", Default = getgenv().velocityEnabled})

VehicleVelocityToggle:OnChanged(function(Value)
	getgenv().velocityEnabled = Value
end)

local VehicleSlider = Tabs.Vehicle:AddSlider("Slider", {
	Title = "â  Velocity Multiplier",
	Description = "\nChange Vehicle Velocity Multiplier",
	Default = 25,
	Min = 1,
	Max = 200,
	Rounding = 1,
	Callback = function(Value)
		getgenv().velocityMulti = Value/1000
	end
})


local Keybind = Tabs.Vehicle:AddKeybind("VehicleVelocityKeyBind", {
	Title = "âš¡  Velocity Bind",
	Mode = "Hold",
	Default = "W",

	Callback = function(Value)
		if not getgenv().velocityEnabled then
			return
		end
		while Value == true do
			task.wait(0)
			local Character = game.Players.LocalPlayer.Character
			if Character and typeof(Character) == "Instance" then
				local Humanoid = Character:FindFirstChildWhichIsA("Humanoid")
				if Humanoid and typeof(Humanoid) == "Instance" then
					local SeatPart = Humanoid.SeatPart
					if SeatPart and typeof(SeatPart) == "Instance" and SeatPart:IsA("VehicleSeat") then
						SeatPart.AssemblyLinearVelocity *= Vector3.new(1 + getgenv().velocityMulti, 1, 1 + getgenv().velocityMulti)
					end
				end
			end
			if not getgenv().velocityEnabled then
				break
			end
		end
	end
})

