    Title = "ğŸ’¨  Dash  ğŸ’¨"
})

function Info(country_emoji, device_emoji)
	Tabs.Home:AddParagraph({
		Title = "ğŸ‘¤  User  ğŸ‘¤",
		Content = "\n\n ğŸ¡  UserName : "..UserName.."\n\n ğŸ’¬  Display Name : "..DisplayName.."\n\n ğŸ’³  UserId : "..UserId.."\n\n "..country_emoji.."  Country : "..Country.."\n\n ğŸ’‰  Executor : "..identifyexecutor().."\n\n "..device_emoji.."  Device : "..Device.."\n"
	})
end

if Country == "RU" then
